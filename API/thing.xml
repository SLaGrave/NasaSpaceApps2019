<GDAL_WMS>
    <Service name="TMS">
        <ServerUrl>https://gibs.earthdata.nasa.gov/wmts/epsg4326/best/Landsat_WELD_CorrectedReflectance_TrueColor_Global_Annual/default/2010-12-01/31.25m/${z}/${y}/${x}.jpg</ServerUrl>
    </Service>
    <DataWindow>
        <TileLevel>13</TileLevel>
        <YOrigin>default</YOrigin>
    </DataWindow>
    <Projection>EPSG:4326</Projection>
    <BlockSizeX>512</BlockSizeX>
    <BlockSizeY>512</BlockSizeY>
    <BandsCount>3</BandsCount>
    <ZeroBlockHttpCodes>400</ZeroBlockHttpCodes>
</GDAL_WMS>
